    <!-- start-smoth-scrolling -->
    <script type="text/javascript" src="~/Content/js/move-top.js"></script>
    <script type="text/javascript" src="~/Content/js/easing.js"></script>
    <script type="text/javascript">
	jQuery(document).ready(function($) {
		$(".scroll").click(function(event){
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
		});
	});
    </script>
    <!-- start-smoth-scrolling -->
</head>

<!-- //navigation -->
<!-- breadcrumbs -->
<div class="breadcrumbs">
    <div class="container">
        <ol class="breadcrumb breadcrumb1 animated wow slideInLeft" data-wow-delay=".5s">
            <li><a href="/Home/Index"><span class="glyphicon glyphicon-home" aria-hidden="true"></span>Главная страница</a></li>
            <li class="active">Страница регистрации</li>
        </ol>
    </div>
</div>
<!-- //breadcrumbs -->
<!-- register -->
<div class="register">
    <div class="container">
        <h2>Форма регистрации</h2>
       <h3  style="text-align:center"><span class="label-warning">@ViewBag.mess</span></h3>
        <div class="login-form-grids">
            <h5>Информация профиля</h5>
            <form action="@Url.Action("Registered","Home")" method="post">
                <input type="text" name="name" placeholder="Имя...">
                <input type="text" name="lastname"  placeholder="Фамилия...">
                <input type="text" name="number" placeholder="Номер телефона..."><br />
                <input type="text" name="address" placeholder="Ваш адресс..."><br />
                <input type="text" name="login" placeholder="логин">
                <input type="password" name="parol" placeholder="пароль">
                <input type="password" name="povparol" placeholder="подтверждение пароля">
                <div class="register-check-box">
                    <div class="check">
                        <label class="checkbox"><input type="checkbox" name="checkbox"><i> </i>Я принимаю правила и условия</label>
                    </div>
                </div>
                <input type="submit" value="Register">
            </form>
        </div>
        <div class="register-home">
            <a href="/Home/Index">Главная страница</a>
        </div>
    </div>
</div>
<!-- //register -->
<!-- //footer -->

